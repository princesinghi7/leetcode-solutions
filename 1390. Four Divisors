class Solution {
public:
    int sumFourDivisors(vector<int>& nums) {
        int ans = 0;
        for(int x : nums) {
            int cnt = 0, sum = 0;
            for(int d = 1; d * d <= x; d++) {
                if(x % d == 0) {
                    int a = d, b = x / d;
                    if(a == b) {
                        cnt++;
                        sum += a;
                    } else {
                        cnt += 2;
                        sum += a + b;
                    }
                    if(cnt > 4) break;
                }
            }
            if(cnt == 4) ans += sum;
        }
        return ans;
    }
};
